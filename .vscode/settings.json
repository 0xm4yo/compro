{
    "code-runner.executorMap": {
        "cpp": "cd $dir && g++ $fileName -I $workspaceRoot/ac-library -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "python": "python -u"
    },
    "code-runner.runInTerminal": true,
    "editor.tabCompletion": "on",
    "editor.snippetSuggestions": "top",
    "editor.quickSuggestions": {
        "other": true,
        "comments": true,
        "strings": true
    },
    "files.associations": {
        "iostream": "cpp",
        "*.tcc": "cpp",
        "string": "cpp",
        "unordered_map": "cpp",
        "unordered_set": "cpp",
        "string_view": "cpp",
        "source_location": "cpp",
        "stacktrace": "cpp",
        "bitset": "cpp",
        "format": "cpp"
    },
}
